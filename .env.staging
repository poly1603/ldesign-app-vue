# 预发布环境变量

# Node 环境
NODE_ENV=production

# API 基础地址
VITE_API_BASE_URL=https://staging-api.example.com

# WebSocket 地址
VITE_WS_URL=wss://staging-api.example.com/ws

# 是否启用 Mock 数据
VITE_ENABLE_MOCK=false

# 是否启用调试
VITE_ENABLE_DEBUG=true

# 日志级别 (debug | info | warn | error)
VITE_LOG_LEVEL=info

# 是否启用性能监控
VITE_ENABLE_PERFORMANCE=true

# 是否启用错误追踪
VITE_ENABLE_ERROR_TRACKING=true

# Sentry DSN（错误追踪）
VITE_SENTRY_DSN=

# Google Analytics ID（测试）
VITE_GA_ID=

# CDN 地址
VITE_CDN_URL=https://staging-cdn.example.com

# 是否启用 CDN
VITE_ENABLE_CDN=false

# 是否启用代码压缩
VITE_ENABLE_MINIFY=true

# 是否生成 sourcemap
VITE_ENABLE_SOURCEMAP=true

